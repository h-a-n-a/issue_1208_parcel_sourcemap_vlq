#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/bin/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/.pnpm/typescript@4.5.2/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/.pnpm/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules:/Users/bytedance/Desktop/Projects/issues/node_modules:/Users/bytedance/Desktop/Projects/node_modules:/Users/bytedance/Desktop/node_modules:/Users/bytedance/node_modules:/Users/node_modules:/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/typescript/bin/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/typescript/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/bin/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/.pnpm/typescript@4.5.2/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/.pnpm/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules:/Users/bytedance/Desktop/Projects/issues/node_modules:/Users/bytedance/Desktop/Projects/node_modules:/Users/bytedance/Desktop/node_modules:/Users/bytedance/node_modules:/Users/node_modules:/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/typescript/bin/node_modules:/Users/bytedance/Desktop/Projects/issues/issue_1208_parcel_sourcemap_vlq/node_modules/typescript/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../typescript/bin/tsc" "$@"
fi
